
srcdir        = @srcdir@
VPATH         = @srcdir@

# DO NOT INCLUDE CMakeMaster.make here!
# This will cause an infinite loop as it will add the
# command for changing into this directory

# let cmake know that this was done with autoconf
KIT_FLAGS = -DCMAKE_HAS_AUTOCONF  -I${CMAKE_CONFIG_DIR}/CMake/Source 
include @CMAKE_CONFIG_DIR@/CMake/CMakeVariables.make
include @CMAKE_CONFIG_DIR@/CMake/CMakeSimpleRules.make

OBJS = \
CMakeBuildTargets.o  \
cmMakeDepend.o \
cmMakefile.o \
cmMakefileGenerator.o \
cmRegularExpression.o \
cmSourceFile.o \
cmSystemTools.o \
cmDirectory.o \
cmUnixMakefileGenerator.o \
cmCommands.o \
cmTarget.o \
cmCustomCommand.o \
cmCacheManager.o \
cmSourceGroup.o

DEPENDS = $(srcdir)/*.h  ${CMAKE_CONFIG_DIR}/CMake/Source/cmConfigure.h

cmCollectFlags.o : $(DEPENDS)
CMakeBuildTargets.o  : $(DEPENDS)
cmMakeDepend.o : $(DEPENDS)
cmMakefile.o : $(DEPENDS)
cmMakefileGenerator.o : $(DEPENDS)
cmAuxSourceDirectoryCommand.o : $(DEPENDS)
cmRegularExpression.o : $(DEPENDS)
cmSourceFile.o : $(DEPENDS)
cmDirectory.o : $(DEPENDS)
cmCustomCommand.o : $(DEPENDS)
cmUnixMakefileGenerator.o : $(DEPENDS)
cmCommands.o : $(DEPENDS) $(srcdir)/*Command*.cxx
cmTarget.o : $(DEPENDS)
cmCacheManager.o : $(DEPENDS)
cmSourceGroup.o : $(DEPENDS)



CMakeBuildTargets: ${OBJS}
	${CXX} ${OBJS} ${CXX_FLAGS}  -o CMakeBuildTargets

